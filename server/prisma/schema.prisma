generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  username String
  password String
}

model Game {
  id          Int           @id @default(autoincrement())
  name        String
  GameVersion GameVersion[]
}

model GameVersion {
  id           Int    @id @default(autoincrement())
  gameId       Int
  game         Game   @relation(fields: [gameId], references: [id])
  version      String
  downloadLink String
}
